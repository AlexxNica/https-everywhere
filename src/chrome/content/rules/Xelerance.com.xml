<ruleset name="Xelerence.com" platform="cacert">

	<target host="xelerance.com" />
	<target host="*.xelerance.com" />


	<rule from="^http://(?:www\.)?xelerance\.com/"
		to="https://www.xelerance.com/" />

	<rule from="^http://dane\.xelerence\.com/"
		to="https://dane.xelerence.com/" />

</ruleset>